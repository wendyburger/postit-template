
<% if @post.errors.any? %>
  <div class="alert alert-error">
  <h5>There were some errors:</h5>
  <ul>
    <% @post.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  </ul>
  </div>
<% end %>


<%= form_for @post do |f| %>
  <%= f.label :title%>
  <%= f.text_field :title%>
  </br>
  <%= f.label :url%>
  <%= f.text_field :url%>
  </br>
  <%= f.submit "Create post", class: "btn btn-primary" %>
<% end %>
